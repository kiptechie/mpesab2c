{
    "name": "manuelgeek/mpesab2c",
    "description": "Safaricom's Mpesa Daraja API B2C implementation",
    "keywords": ["mpesa", "safaricom", "laravel", "transactions", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "manuelgeek",
            "email": "emashmagak@appslab.co.ke"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Manuelgeek\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manuelgeek\\MpesaB2C\\MpesaServiceProvider"
            ],
            "aliases": {
                "B2C": "Manuelgeek\\MpesaB2C\\Facades\\B2C"
            }
        }
    }
}
